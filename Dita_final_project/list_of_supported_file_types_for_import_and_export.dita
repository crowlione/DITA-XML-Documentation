                    <row>
                        <entry>Animated PNG</entry>
                        <entry>No</entry>
                        <entry>Yes</entry>
                    </row>
                    <row>
                        <entry>Animated MP4</entry>
                        <entry>No</entry>
                        <entry>Yes</entry>
                    </row>
                    <row>
                        <entry>Animated HEVC</entry>
                        <entry>No</entry>
                        <entry>Yes</entry>
                    </row>
                </tbody>
